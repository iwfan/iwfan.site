---
import Heart from "../components/Heart.astro"
import Snazzy from "../components/Snazzy.astro"
import Layout from "../layouts/Layout.astro"

const list = [
  {
    title: "adwadawdaw",
    date: "2023-03-15",
  },
  {
    title: "adwadawdaw",
    date: "2023-03-15",
