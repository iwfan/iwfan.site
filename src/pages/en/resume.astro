---
import { getEntry } from "astro:content"
import Layout from "../../layouts/Layout.astro"

const entry = await getEntry("docs", "resume")
const { Content } = await entry.render()
---

<Layout title="Resume" desc="iwfan's personal resume." classes={["container px-4 font-sans"]}>
  <article class="prose prose-invert w-full max-w-screen-md flex-none mx-auto">
    <h1 class="font-bold font-jost text-xl py-6 md:text-2xl lg:text-3xl my-4">Resume</h1>
    <Content />
  </article>
</Layout>
